            <vbaCode>
                <![CDATA[Private Sub Display_AnimationStart()

On Error GoTo ErrHandler

Dim INSTRUMENTO As String
Dim INSTRUMENTO2 As String

INSTRUMENTO = Text1.Caption
INSTRUMENTO2 = Text36.Caption
PONTEIRO_1 = Mid(INSTRUMENTO, 4, 1)
PONTEIRO_2 = Mid(INSTRUMENTO, 12, 1)
PONTEIRO2_1 = Mid(INSTRUMENTO2, 4, 1)
PONTEIRO2_2 = Mid(INSTRUMENTO2, 12, 1)

If PONTEIRO_1 = "-" Then
     INSTR_TIPO = Left(INSTRUMENTO, 1)
        If PONTEIRO_2 <> "-" Then
            INSTR_NUMERO = Mid(INSTRUMENTO, 5, 8)
             Else
                INSTR_NUMERO = Mid(INSTRUMENTO, 5, 7) + "_" + Mid(INSTRUMENTO, 13, 3)
        End If
    Else
     INSTR_TIPO = Left(INSTRUMENTO, 2)
     INSTR_NUMERO = Mid(INSTRUMENTO, 6, 8)
End If

If PONTEIRO2_1 = "-" Then
     INSTR_TIPO2 = Left(INSTRUMENTO2, 1)
        If PONTEIRO2_2 <> "-" Then
            INSTR_NUMERO2 = Mid(INSTRUMENTO2, 5, 8)
             Else
                INSTR_NUMERO2 = Mid(INSTRUMENTO2, 5, 7) + "_" + Mid(INSTRUMENTO2, 13, 3)
        End If
    Else
     INSTR_TIPO2 = Left(INSTRUMENTO2, 2)
     INSTR_NUMERO2 = Mid(INSTRUMENTO2, 6, 8)
End If

If INSTR_TIPO = "L" Then
        AlarmEventSummary1.FilterDefinition = "EventSource LIKE '" & INSTR_TIPO & "' And (EventSource LIKE '" & INSTR_NUMERO & "' Or EventSource LIKE '" & INSTR_NUMERO2 & "') And (EventSource LIKE 'H_' Or EventSource LIKE 'L_')"
        ' FactoryTalkAlarmandEventLogViewer1.WhereClause = " SourceName LIKE '%" & INSTR_TIPO & "%' And (SourceName LIKE '%" & INSTR_NUMERO & "%' Or SourceName LIKE '%" & INSTR_NUMERO2 & "%') And (SourceName LIKE '%LA%H_%' Or SourceName LIKE '%LA%L_%')"
    Else
        AlarmEventSummary1.FilterDefinition = "EventSource LIKE '" & INSTR_TIPO & "' And (EventSource LIKE '" & INSTR_NUMERO & "' Or EventSource LIKE '" & INSTR_NUMERO2 & "')"
        ' FactoryTalkAlarmandEventLogViewer1.WhereClause = " SourceName LIKE '%" & INSTR_TIPO & "%' And SourceName LIKE '%" & INSTR_NUMERO & "%'"
End If

Exit Sub
ErrHandler:

End Sub
]]>
