                <![CDATA[Private Sub BtnConfirma_Released()
    If GrupoComentario.Visible Then
        If CaixadeTexto1.Value <> "" Then
            comentario = CaixadeTexto1.Value
            ftcommand = "Remark OVERRIDE+" & Me.TagParameters(1) & "+$" & Me.TagParameters(1) & "$+" & comentario & ";&Toggle " & Me.TagParameters(1) & ";"
            Application.ExecuteCommand (ftcommand)
            Me.Unload
        Else
            result = MsgBox("Favor inserir motivo de ativação do override!", vbInformation)
        End If
    Else
        ftcommand = "Remark OVERRIDE+" & Me.TagParameters(1) & "+$" & Me.TagParameters(1) & "$+" & comentario & ";&Toggle " & Me.TagParameters(1) & ";"
        Application.ExecuteCommand (ftcommand)
        Me.Unload
    End If
        
