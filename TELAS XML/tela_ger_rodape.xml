            <vbaCode>
                <![CDATA[Option Explicit

Private Declare Sub keybd_event Lib "user32" (ByVal bVk As Byte, ByVal bScan As Byte, ByVal dwFlags As Long, ByVal dwExtraInfo As Long)
Private Const KEYEVENTF_KEYUP = &H2
Private Const VK_SNAPSHOT = &H2C
Private Const VK_MENU = &H12

Private Const PastaPrntScrn = "c:\printscreen\"

Private Sub BotaoPrintScreen_Released()
    Dim Calendario As String
    Dim Arquivo As String
    Dim clip As Object
    Dim fs As Object
    
    On Error GoTo ErrHandler
    
    Set clip = CreateObject("clipbrd.clipboard")
    Set fs = CreateObject("Scripting.FileSystemObject")
    
    'Define a formatação da data e hora
    Calendario = Format(Date, "yyyy.mm.dd_") & Format(Time, "hh.mm.ss")
    'Define o nome do arquivo
    Arquivo = PastaPrntScrn & Calendario & ".bmp"
    'Verifica se a pasta onde os backups serão salvos existe
    If fs.FolderExists(PastaPrntScrn) = False Then
        'Exibe aviso
        VBA.MsgBox "A pasta " & PastaPrntScrn & " não está acessível." & VBA.vbCrLf & _
            "O arquivo " & Arquivo & " não poderá ser gravado.", _
            VBA.vbOKOnly + VBA.vbExclamation, "Erro"
        'Destrói objeto
        Set fs = Nothing
        'Sai da sub-rotina
        Exit Sub
    End If
    'Comandos equivalentes ao "ALT + PrintScreen"
    keybd_event VK_MENU, 0, 0, 0
    keybd_event VK_SNAPSHOT, 0, 0, 0
    DoEvents
    keybd_event VK_SNAPSHOT, 0, KEYEVENTF_KEYUP, 0
    keybd_event VK_MENU, 0, KEYEVENTF_KEYUP, 0
    'Salva a tela do Clipboard para o arquivo "bmp"
    SavePicture clip.GetData, Arquivo
    'Exibe aviso informando que o arquivo foi salvo
    VBA.MsgBox _
        "Arquivo Salvo com Sucesso!" & vbCrLf & vbCrLf & _
        "Arquivo: " & Arquivo & vbCrLf & _
        "Pasta: " & PastaPrntScrn
    'Destrói objeto
    Set clip = Nothing
    'Sai da sub-rotina
    Exit Sub
ErrHandler:
    'Destrói objetos
    Set clip = Nothing
    Set fs = Nothing
    LogDiagnosticsMessage "VBA Error (SetServerStatus()): " & Hex(Err.Number) & " - " & Err.Description, ftDiagSeverityError
End Sub


]]>
