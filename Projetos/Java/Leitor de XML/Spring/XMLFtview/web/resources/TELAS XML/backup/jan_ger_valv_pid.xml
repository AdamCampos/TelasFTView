                <![CDATA[Private Sub AbaAlmHistorico_Click()

On Error GoTo ErrHandler

Dim INSTRUMENTO As String
Dim TIPO As String

INSTRUMENTO = Replace(Text1.Caption, "-", "_")
PONTEIRO = Mid(INSTRUMENTO, 3, 1)

If PONTEIRO = "_" Then
' Para todos os intrumentos (exceto PDITs)

    ALM_1 = Left(INSTRUMENTO, 1)
    ALM_2 = Left(INSTRUMENTO, 3)
    NUMERO = Mid(INSTRUMENTO, 4)
Else
' Para PDITs (
    ALM_1 = Left(INSTRUMENTO, 2) & "A"
    ALM_2 = Left(INSTRUMENTO, 4)
    NUMERO = Mid(INSTRUMENTO, 6)

End If


' AlarmEventSummary1.FilterDefinition = " EventSource LIKE '" & NUMERO & "' And (EventSource LIKE '" & ALM_1 & "' Or EventSource LIKE '" & ALM_2 & "')"
FactoryTalkAlarmandEventLogViewer1.WhereClause = " SourceName LIKE '%" & NUMERO & "%' And (SourceName LIKE '%" & ALM_1 & "%' Or SourceName LIKE '%" & ALM_2 & "%')"


Exit Sub
ErrHandler:


End Sub

