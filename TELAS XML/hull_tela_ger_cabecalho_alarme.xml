            <vbaCode>
                <![CDATA[Option Explicit

'Declaração das variáveis globais
Dim bitAlarme1 As Boolean
Dim bitAlarme2 As Boolean
Dim ValorAlarme1 As Integer
Dim ValorAlarme2 As Integer

'Inicia a tela
Private Sub Display_AnimationStart()
    'Verifica erro
    On Error GoTo ErrHandler
    
    bitAlarme1 = False
    bitAlarme2 = False
    ValorAlarme1 = 0
    ValorAlarme2 = 1
    Image_alarm_ack_no_sel.Visible = True
    Image_alarm_ack_sel.Visible = False
    Image_alarm_silence_all.Visible = True
    Image_alarm_silence_no_all.Visible = False
    
    'Sai da sub-rotina
    Exit Sub
ErrHandler:
    'Registra o erro
    LogDiagnosticsMessage "VBA Error (Display_AnimationStart()): " & Hex(Err.Number) & " - " & Err.Description, ftDiagSeverityError
End Sub

'Lógica do Botão Reconhece Alarme
Private Sub BotaoReconhece_Released()
    'Verifica erro
    On Error GoTo ErrHandler
    
    If bitAlarme2 = True Then
        bitAlarme2 = False
        Image_alarm_ack_no_sel.Visible = True
        Image_alarm_ack_sel.Visible = False
    End If

    'Sai da sub-rotina
    Exit Sub
ErrHandler:
    'Registra o erro
    LogDiagnosticsMessage "VBA Error (BotaoReconhece_Released()): " & Hex(Err.Number) & " - " & Err.Description, ftDiagSeverityError
End Sub

'Lógica do Botão Silência Alarme
Private Sub BotaoSilencia_Released()
    'Verifica erro
    On Error GoTo ErrHandler
    
    If bitAlarme1 = False Then
        bitAlarme1 = True
        Image_alarm_silence_no_all.Visible = True
        Image_alarm_silence_all.Visible = False
    Else
        bitAlarme1 = False
        Image_alarm_silence_all.Visible = True
        Image_alarm_silence_no_all.Visible = False
    End If
    
    'Sai da sub-rotina
    Exit Sub
ErrHandler:
    'Registra o erro
    LogDiagnosticsMessage "VBA Error (BotaoSilencia_Released()): " & Hex(Err.Number) & " - " & Err.Description, ftDiagSeverityError
End Sub

'Lógica do Sumário de Alarmes quando for clicado
Private Sub AlarmEventSummary1_EventClick(ByVal Item As FTAlarmEventSummary.IEventItem)
    'Verifica erro
    On Error GoTo ErrHandler

    If Numeric_Variacao.Value > 0 Then
        bitAlarme2 = True
        Image_alarm_ack_sel.Visible = True
        Image_alarm_ack_no_sel.Visible = False
    Else
        bitAlarme2 = False
        Image_alarm_ack_no_sel.Visible = True
        Image_alarm_ack_sel.Visible = False
    End If

    'Sai da sub-rotina
    Exit Sub
ErrHandler:
    'Registra o erro
    LogDiagnosticsMessage "VBA Error (AlarmEventSummary1_EventClick()): " & Hex(Err.Number) & " - " & Err.Description, ftDiagSeverityError
End Sub

'Lógica de variação dos Alarmes Não Reconhecidos para executar Beep conforme a tag system\BlinkFast
Private Sub Numeric_Pulso_Change()
    'Verifica erro
    On Error GoTo ErrHandler
    
    If bitAlarme1 = False Then
        ExecuteCommand ("Beep")
    End If

    'Sai da sub-rotina
    Exit Sub
ErrHandler:
    'Registra o erro
    LogDiagnosticsMessage "VBA Error (Numeric_Pulso_Change()): " & Hex(Err.Number) & " - " & Err.Description, ftDiagSeverityError
End Sub

'Lógica para executar o Beep conforme quantidade de Alarmes Não Reconhecidos
Private Sub Numeric_Variacao_Change()
    'Verifica erro
    On Error GoTo ErrHandler

    ValorAlarme1 = Numeric_Variacao.Value
    
    If ValorAlarme2 <= ValorAlarme1 Then
        ValorAlarme2 = ValorAlarme1
        bitAlarme1 = False
        Image_alarm_silence_all.Visible = True
        Image_alarm_silence_no_all.Visible = False
    End If

    If ValorAlarme1 = 0 Then
        ValorAlarme2 = 1
        bitAlarme1 = False
        bitAlarme2 = False
        Image_alarm_ack_no_sel.Visible = True
        Image_alarm_ack_sel.Visible = False
        Image_alarm_silence_all.Visible = True
        Image_alarm_silence_no_all.Visible = False
    End If
    
    'Sai da sub-rotina
    Exit Sub
ErrHandler:
    'Registra o erro
    LogDiagnosticsMessage "VBA Error (Numeric_Variacao_Change()): " & Hex(Err.Number) & " - " & Err.Description, ftDiagSeverityError
End Sub
Private Sub Numeric_VariacaoTopo_Change()
    
    'Move o cursor para o topo na entrada de um novo alarme
    ExecuteCommand ("Pause 2;Invoke tela_ger_cabecalho_alarme.AlarmEventSummary1.MoveSelectionFirst")
    
End Sub
